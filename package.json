  "keywords": [
    "react",
    "typescript",
    "helpers",
    "utils",
    "react-utils",
    "react-helpers"
  ],
  "scripts": {
    "build": "rm -rf dist/ && tsc ./src/index.ts --esModuleInterop --outDir ./dist",
